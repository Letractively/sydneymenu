<?xml version="1.0" encoding="UTF-8"?>
{% if ERROR %}
<FAIL>
  {% for key, value in ERROR.iteritems%}
  <ERROR name='{{key}}'>{{value}}
  </ERROR>
  {% endfor %}
  <HTMLMSG>
  {{MSG}}
  </HTMLMSG>
</FAIL>
{% else %}
<SUCC timestamp='{{TIME_STAMP}}'>
  <HTMLMSG>
  {{MSG}}
  </HTMLMSG>
</SUCC>
{% endif %}
